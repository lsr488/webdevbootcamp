<% include partials/header.ejs %>

<h1>Index Page</h1>

<% blogs.forEach(function(blog) { %>
  <div>
    <h2>
      <%= blog.title %>
      <img src="<%= blog.image %>">
      <span><%= blog.created %></span>
      <p><%= blog.body %></p>
    </h2>
  </div>
<% }); %>

<% include partials/footer.ejs %>